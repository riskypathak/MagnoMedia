@model IEnumerable<MagnoMedia.Data.Models.ThirdPartyApplication>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@*<p>
    @Html.ActionLink("Create New", "Add")
   @Html.ActionLink("Log Off", "LogOff", "User")
</p>*@

<h2>Third Party Application List</h2>
<table class="table table-striped table-bordered">
    <tr>
        <th>
            @Html.DisplayName("Name")
        </th>
        <th>
            @Html.DisplayName("Installer Name")
        </th>
        <th>
            @Html.DisplayName("Arguments")
        </th>
        <th>
            @Html.DisplayName("Download Url")
        </th>
        <th>Action</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td style="width: 120px">
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td style="width: 150px">
                @Html.DisplayFor(modelItem => item.InstallerName)
            </td>
            <td style="width: 200px">
                @Html.DisplayFor(modelItem => item.Arguments)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DownloadUrl)
            </td>

            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id })
                <br />
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>



