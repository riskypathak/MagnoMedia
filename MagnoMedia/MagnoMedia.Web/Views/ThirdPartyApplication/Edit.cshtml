@model MagnoMedia.Web.DataModel.thirdpartyapplication

@{
    ViewBag.Title = "Edit";
}
<p>
   
    @Html.ActionLink("Log Off", "LogOff","User")
</p>
@using (Html.BeginForm("Edit", "thirdpartyapplication", FormMethod.Post))
{
    <fieldset>
        <legend>Edit Application</legend>
        <table style="width: 50%; padding: 30px;">
            <tr>
                <td style="width: 100px">Name :</td>
                <td>@Html.TextBoxFor(e => e.Name)</td>
            </tr>
            <tr>
                <td style="width: 100px">Installer Name :</td>
                <td>@Html.TextBoxFor(e => e.InstallerName)</td>
            </tr>
            <tr>
                <td style="width: 100px">Arguments :</td>
                <td>@Html.TextBoxFor(e => e.Arguments)</td>
            </tr>
            <tr>
                <td style="width: 100px">Download Url :</td>
                <td>@Html.TextBoxFor(e => e.DownloadUrl)</td>
            </tr>
            <tr>
                <td style="width: 100px">Url :</td>
                <td>@Html.TextBoxFor(e => e.Url)</td>
            </tr>

            <tr>
                <td></td>
                <td>
                    <input type="submit" name="name" value="Save Changes" />
                </td>
            </tr>

        </table>
    </fieldset>

}
<style>
    tr {
        padding: 10px 0px;
    }
    tr td:first-child {
        text-align: right;
        padding-right: 20px;
        width:30%;
    }
    input[type="text"]
    {
        width:400px !important;
    }
</style>