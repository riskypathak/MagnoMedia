                            <li><a href="#">Installer</a></li>
                            <li class="divider"></li>
                            <li><a href="#">Appications</a></li>
                            <li class="divider"></li>
                            <li><a href="/MetricAndEfficency/Index">Metrics</a></li>

                        </ul>
                    </li>
                    <li><a href="../User/LogOff">Log Out</a></li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="container">
        @RenderBody()
